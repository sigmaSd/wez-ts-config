{
  "include": ["src"],
  "compilerOptions": {
       "module": "commonjs",
    "target": "esnext",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "strict": true,
    "types": ["@typescript-to-lua/language-extensions", "lua-types/jit"],
  },
  "tstl": {
    "luaTarget": "JIT",
    "noImplicitSelf": true
  }
}
